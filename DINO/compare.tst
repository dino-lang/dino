import sys

def main():
    dict = {}
    for line in open("__temp").xreadlines():
        word = line[:-1]
        if word: dict[word] = 1

    for line in sys.stdin.xreadlines():
        word = line[:-1]
        if word:
            if not (word in dict): print word

main()
