#!/bin/sh
# @configure_input@

#
# Script to test of arithmetic package (files `arithm.h', `arithm.cpp').
#

#
# Usage: arithm++.tst [start_test_number]
#
# The script outputs self-explanatory messages and returns zero exit code if 
# it is all ok.
#

script_file=$0
CXX='@CXX@'
CXX_options='@CXXFLAGS@ @DEFS@ -I. -I@srcdir@/'
source_addfiles='@srcdir@/arithm.cpp'
addfiles='arithm.o'
test_file=tarithm.cpp
outfile=./a.out

if test x$1 = x
then
    start_test_number=1
else
    if expr $1 + 0 >/dev/null
    then
        start_test_number=`expr $1 + 0`
    else
        echo $script_file:invalid argument $1 2>&1
        exit 1
    fi
fi

# Compilation of addfiles

echo '      ' $CXX $CXX_options -c $source_addfiles
if $CXX $CXX_options -c $source_addfiles; then
        result=ok
else
        echo '***' $script_file: test is failed see file $test_file
        exit 1
fi

# Test 1.
if test $result = ok -a $start_test_number -le 1; then
        cat >$test_file <<'TEST1'

#ifdef HAVE_CONFIG_H
#include "cocom-config.h"
#else /* In this case we are oriented to ANSI C */
#ifndef HAVE_LIMITS_H
#define HAVE_LIMITS_H
#endif
#endif /* #ifdef HAVE_CONFIG_H */


#include <stdlib.h>
#include <stdio.h>
#include <string.h>
#include "arithm.h"

#ifdef HAVE_LIMITS_H
#include <limits.h>
#else
