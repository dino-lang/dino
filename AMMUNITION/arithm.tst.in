  if (overflow_bit)
    exit (1);
  integer_to_string (4, d, s);
  if (strcmp (s, str) !=0)
    exit (1);

  integer_from_based_string (4, "-80000001", 16, d);
  if (!overflow_bit)
    exit (1);
  sprintf (str, "%d", INT_MIN);
  integer_from_string (4, str, d);
  if (overflow_bit)
    exit (1);
