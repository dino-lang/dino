  integer_to_based_string (4, d, 8, s);
  if (strcmp (s, "17777777777") !=0)
    exit (1);  

  for (i = -2000; i < 2000 ; i++)
    {
      sprintf (str, "%d", i);
      integer_from_string (4, str, d);
      if (overflow_bit)
        exit (1);
      integer_to_string (4, d, s);
      if (strcmp (s, str) !=0)
        exit (1);
    }
  for (b = 2; b <= 36; b++)
    for (i = -2000; i < 2000; i++)
      {
        sprintf (str, "%d", i);
        integer_from_string (4, str, d);
        if (overflow_bit)
          exit (1);
        integer_to_based_string (4, d, b, s);
        integer_from_based_string (4, s, b, d1);
        if (!eq_integer (4, d, d1))
          exit (1);
      }
  exit (0);
}
TEST1

