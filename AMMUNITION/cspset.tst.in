#include "cspset_temp.h"
#include "ticker.h"
#ifdef linux
#include <unistd.h>
#endif

int n;
main ()
{
  cspset_t s1, s2, s3, s4;
  int i, iter;
  ticker_t t = create_ticker ();
#ifdef linux
  char *start = sbrk (0);
#endif

  for (iter = 0; iter < 1000; iter++)
    {
      cspset_init (&s1, 0);
      cspset_init (&s2, 0);
      cspset_init (&s3, 0);
      cspset_init (&s4, 0);
      if (cspset_diff (&s2, &s1))
        exit (1);
      check (&s2, NULL, 0);
      check (&s1, NULL, 0);
      for (i = 0; i < 1000; i++)
        cspset_insert (&s1, random () % 10000);
      for (i = 0; i < 10000; i++)
        if (cspset_in_p (&s1, i))
          n++;
      for (i = 0; i < 10000; i += 3)
        cspset_remove (&s1, i);
      cspset_copy (&s3, &s1);
      for (i = 0; i < 1000; i++)
        cspset_insert (&s2, random () % 10000);
      cspset_copy (&s4, &s2);
      cspset_intersect (&s4, &s1);
      for (i = 0; i < 10; i++)
       {
         cspset_unity (&s1, &s2);
         cspset_diff (&s1, &s2);
         cspset_unity (&s1, &s4);
         cspset_shrink (&s1);
         if (! cspset_equal_p (&s1, &s3))
           exit (1);
      }
      cspset_release_unused_memory ();
      cspset_copy (&s4, &s1);
      cspset_diff (&s4, &s2);
      for (i = 0; i < 10; i++)
       {
         cspset_intersect (&s1, &s2);
         cspset_unity (&s1, &s4);
         cspset_shrink (&s1);
         if (! cspset_equal_p (&s1, &s3))
           exit (1);
      }
      cspset_finish (&s1);
      cspset_finish (&s2);
      cspset_finish (&s3);
      cspset_finish (&s4);
      cspset_release_unused_memory ();
    }
#ifndef NDEBUG
#ifdef linux
  printf ("all time %.2f, memory=%.1fkB, searches=%d, collisions=%d\n",
          active_time (t), ((char *) sbrk (0) - start) / 1024.,
          cspset_all_searches, cspset_all_collisions);
#else
  printf ("all time %.2f, searches=%d, collisions=%d\n", active_time (t),
          cspset_all_searches, cspset_all_collisions);
#endif
#else
#ifdef linux
  printf ("all time %.2f, memory=%.1fkB\n",
          active_time (t), ((char *) sbrk (0) - start) / 1024.);
